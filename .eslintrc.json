{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "import/no-extraneous-dependencies": "off",
    "class-methods-use-this": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "off",
    "no-param-reassign": "off"
  }
}
